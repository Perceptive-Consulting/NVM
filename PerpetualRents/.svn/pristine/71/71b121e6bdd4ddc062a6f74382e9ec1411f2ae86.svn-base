package com.pcs.perpetualRents.enm;

public enum PDDocumentsType {

	UNDEFINED(0, "Undefined"),
	DRAWINGS(1, "Drawings"),
	SPECIFICATION(2, "Specification"),
	LAND_REGISTRY(3, "Land Registry"),
	TENDER_DOCUMENTS(4, "Tender Documents"),
	CONSTRUCTION_SCHEDULE(5, "ConstructionSchedule"),
	ADDITIONAL_DOCUMENTS_1(6, "Additional Documents 1"),
	ADDITIONAL_DOCUMENTS_2(7, "Additional Documents 2"),
	ADDITIONAL_DOCUMENTS_3(8, "Additional Documents 3"),
	ADDITIONAL_DOCUMENTS_4(9, "Additional Documents 4"), 
	ADDITIONAL_DOCUMENTS_5(10, "Additional Documents 5"),
	IMAGES(11, "Images"),
	ADMIN_UPLOADED_DOCS(12, "Admin Uploaded Documents");

	private final long id;
	private final String value;

	PDDocumentsType(long idx, String value) {
		this.id = idx;
		this.value = value;
	}
	
	public long id() {
		return id;
	}

	public String value() {
		return value;
	}
}
